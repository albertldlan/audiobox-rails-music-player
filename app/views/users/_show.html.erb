<div class="user-show-page container">
	
	<div id="user-show-header">
		<h1><%= @user.username %>'s Uploaded Files</h1>
		<% if current_user.id == @user.id %>
		<div id="my-uploads" class="song-list">
			<% else %>
		<div class="song-list">
			<% end %>
			<%= render "/songs/user-songs" %>
		</div>

		<div id="comments-stream">
		
	</div>

</div>